<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Personal Finance Tracker Dashboard</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,0,0" />
    <link rel="stylesheet" href="css/Dashboard.css">
</head>
<body>
    <div class="container">
        <!-- aside section start here -->
        <aside>
            <div class="top">

                <div class="logo">
                    <h2 class="header">Personal<br/><span class="primary">Finance-Tracker</span></h2>

                </div>

                <div class="close">
                    <span class="material-symbols-outlined">
                        close
                    </span>
                </div>

            </div>
             <!-- end top -->

             <div class="sidebar">

                <a href="#" class="active">
                    <span class="material-symbols-outlined">
                        grid_view
                    </span>
                    <h3>Dashboard</h3>
                </a>

                <a href="/Profile">
                    <span class="material-symbols-outlined">
                       account_circle               </span>
                    <h3>Profile</h3>
                </a>

                <a href="/Notification">
                    <span class="material-symbols-outlined">
                      notifications  
                    </span>
                    <h3>Notification</h3>
                    <div class="msg_count">5</div>
                   
                </a>
        
                <a href="#">
                    <span class="material-symbols-outlined">
                        trending_up                    </span>
                    <h3>My Incomes</h3>
                </a>

                <a href="#">
                    <span class="material-symbols-outlined">
                        earthquake                   </span>
                    <h3>My Expenses</h3>
                </a>

                <a href="#">
                    <span class="material-symbols-outlined">
                       lab_profile                    </span>
                    <h3>Reports</h3>
                </a>

                <a href="#">
                    <span class="material-symbols-outlined">
                       settings                  </span>
                    <h3>Settings</h3>
                </a>

                <a href="/">
                    <span class="material-symbols-outlined">
                       Logout               </span>
                    <h3>Log out</h3>
                </a>



             </div>







        </aside>
        <!-- aside section end -->

        <!-- main section start here -->
        <main>
            <h1>Dashboard</h1>

            <div class="date">
                <input type="date">
            </div>

            <div class="insights">


                <!--start incomes-->
                <div class="incomes">
                    <span class="material-symbols-outlined">trending_up</span>
                    <div class="middle">

                        <div class="left">
                            <h3>Total Incomes</h3>
                            <h1>Rs.30,000/-</h1>
                        </div>

                        <div class="progress">
                            <!-- svg ko use le hamile afno hisab le circle create garna sakxam -->
                            <svg> 
                                <circle r="30" cy="40" cx="40"></circle>
                            </svg>
                            <div class="number">
                                80%
                            </div>
                        </div>
                    </div>
                    <small>Last 30 days </small>

                </div>


                <!--end incomes-->

                  <!--start expenses-->
                  <div class="expenses">
                    <span class="material-symbols-outlined">earthquake</span>
                    <div class="middle">

                        <div class="left">
                            <h3>Total Expenses</h3>
                            <h1>Rs.18,000/-</h1>
                        </div>

                        <div class="progress">
                            <svg> <!-- svg ko use le hamile afno hisab le circle create garna sakxam -->
                                <circle r="30" cy="40" cx="40"></circle>
                            </svg>
                            <div class="number">
                                40%
                            </div>
                        </div>

                    </div>

                    <small>Last 30 days </small>    
                </div>


                <!--end expenses-->


                  <!--start balance-->
                  <div class="balanced">
                    <span class="material-symbols-outlined">account_balance</span>
                    <div class="middle">

                        <div class="left">
                            <h3>Net Balance</h3>
                            <h1>Rs.12,000/-</h1>
                        </div>

                        <div class="progress">
                            <svg> <!-- svg ko use le hamile afno hisab le circle create garna sakxam -->
                                <circle r="30" cy="40" cx="40"></circle>
                            </svg>
                            <div class="number">
                                20%
                            </div>
                        </div>

                    </div>
                </div>


                <!--end balance-->

            </div>

            <!-- Ending insights -->

            <!-- start recent order -->

             


             <!-- end recent order -->






            <!-- Saving Goals Start Here -->
            <h2>Savings Goals</h2>
            <div class="savings-goal">
                <div class="goal-details">
                    <h3>Goal 1: Vacation</h3>
                    <p>Save for a vacation to your dream destination.</p>
                    <p>Target Amount: Rs.6000</p>
                </div>
                <div class="goal-progress">
                    <p>Progress:</p>
                    <div class="progress-bar">
                        <div class="savings-progress" style="width: 60%;"></div>
                    </div>
                    <p>60%</p>
                </div>
            </div>
            <div class="savings-goal">
                <div class="goal-details">
                    <h3>Goal 2: Emergency Fund</h3>
                    <p>Build an emergency fund for unexpected expenses.</p>
                    <p>Target Amount:Rs.5000</p>
                </div>
                <div class="goal-progress">
                    <p>Progress:</p>
                    <div class="progress-bar">
                        <div class="savings-progress" style="width: 35%;"></div>
                    </div>
                    <p>35%</p>
                </div>
            </div>
            <!-- Add more savings goals as needed -->




        </main>

        <!-- main section end here -->

        <!-- Right section start here -->
        <div class="right">

            <div class="top">

               

                <div class="theme-toggler">
                    <span class="material-symbols-outlined active">light_mode</span>
                    <span class="material-symbols-outlined">dark_mode</span>
                </div>

                <div class="profile">
                    <div class="info">
                        <p>Admin</p>
                        <small class="text-muted"></small>
                    </div>

                    <div class="profile-photo">
                        <img src="Images/manishSir.jpg">
                    </div>

                </div>

            </div>    
                <!-- Ending top here -->


                <!-- start Income and Expense Source-->
                <div class="income-sources">
                    <h4>Main Income Sources</h4>
                    <ul>
                        <li>
                            <div class="source-details">
                                <h3>Revenue from Digital Pathsala Courses</h3>
                            </div>
                        </li>
                    </ul>
            
                    <ul>
                        <li>
                            <div class="source-details">
                                <h3>Investment Income</h3>
                            </div>
                        </li>
                    </ul>
            
                    <ul>
                        <li>
                            <div class="source-details">
                                <h3>Freelance Work</h3>
                            </div>
                        </li>
                    </ul>
            
                    <ul>
                        <li>
                            <div class="source-details">
                                <h3>Dividend Income</h3>
                            </div>
                        </li>
                    </ul>
            
                <!-- other more income sources details here as needed -->
            
            
                </div>
            
                <div class="expenses-sources">
                    <h4>Major Expenses</h4>
                    <ul>
                        <li>
                            
                            <div class="Expenses-details">
                                <h3>Food Expenses</h3>
                            </div>
                        </li>
                    </ul>
            
                    <ul>
                        <li>
                            <div class="Expenses-details">
                                <h3>Transportation</h3>
                            </div>
                        </li>
                    </ul>
            
                    <ul>
                        <li>
                            <div class="Expenses-details">
                                <h3>Emergency Fund</h3>
                            </div>
                        </li>
                    </ul>
            
                    <ul>
                        <li>
                            <div class="Expenses-details">
                                <h3>Health Care</h3>
                            </div>
                        </li>
                    </ul>
                    <!-- other more expenses details here as needed -->
            
                </div>
                <!-- end recent updates here -->
     
            
            <!-- other analytics coming soon -->    

        </div>
        <!-- Right section end here -->      

    </div>
    
      
   
</body>
</html>
